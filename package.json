{
  "name": "vmolberte",
  "productName": "VMolberte",
  "icon": "./assets/logo256.ico",
  "version": "0.9.3",
  "description": "App for drawing",
  "license": "None",
  "author": "VladimirKirillov",
  "repository": "https://github.com/LeisLoggers/VMolberte",
  "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "start": "electron-forge start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "build": "electron-builder --win",
        "publish": "electron-builder -p always"
    },
  "build": {
    "appId": "org.vmolberte.vmolberte",
    "icon": "./assets/logo256.ico",
    "productName": "VMolberte",
    "files": [
      "index.js",
      "pages/",
      "styles/",
      "assets/**",
      "functions/",
      "fileLoadAndPrimProc.js"
    ],
    "publish": [
        {
            "provider": "github",
            "owner": "LeisLoggers",
            "repo": "VMolberte"
        }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "d3": "^7.9.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "fs": "^0.0.1-security",
    "plotly.js-dist": "^3.1.2",
    "sortablejs": "^1.15.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-forge": "^5.2.4"
  }
}
